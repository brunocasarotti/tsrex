{
  "name": "tsrex",
  "description": "Typescript + React + ExRedux",
  "version": "0.1.1",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist-src/ && npm run build:source && npm run build:binary && npm run build:utils && node ./deploy/post-build.js",
    "pg:start": "node dist-bin/index start ./playground/react.config.js",
    "pg:build": "node dist-bin/index build ./playground/react.config.js",
    "pg:lib": "node dist-bin/index library ./playground/react.config.lib.js",
    "pg:test": "node dist-bin/index test ./playground/react.config.js",
    "pg:static": "node dist-bin/index build ./static-test/react.config.static.js",
    "pg:player": "node dist-bin/index start ./static-test/react.config.player.js",
    "build:source": "tsc -p deploy/tsconfig.source.json",
    "build:binary": "webpack -p --colors --progress --config ./deploy/webpack.config.deploy.js",
    "build:utils": "tsc -d -p deploy/tsconfig.utils.json",
    "deploy": "npm run build && cd dist-bin && npm publish",
    "deploy:next": "npm run build && cd dist-bin && npm publish --tag next"
  },
  "bin": {
    "tsrex": "./index.js"
  },
  "author": "D.A.Paula <deberson.paula@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debersonpaula/tsrex.git"
  },
  "bugs": {
    "url": "https://github.com/debersonpaula/tsrex/issues"
  },
  "homepage": "https://github.com/debersonpaula/tsrex#readme",
